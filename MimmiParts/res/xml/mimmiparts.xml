<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen 
	xmlns:android="http://schemas.android.com/apk/res/android">
	
	<PreferenceCategory
	    android:title="@string/mimmi_title"
	    android:key="parent">

	    <PreferenceScreen
		android:key="brightness"
		android:title="@string/lm3530_head"
		android:summary="@string/lm3530_summary">
		<intent android:action="android.intent.action.MAIN"
            		android:targetPackage="com.cyanogenmod.settings.device"
            		android:targetClass="com.cyanogenmod.settings.device.Brightness" />
	    </PreferenceScreen>
	
	    <PreferenceScreen
		android:key="keyboard"
		android:title="@string/keyboard_head"
		android:summary="@string/keyboard_summary">
		<intent android:action="android.intent.action.MAIN"
            		android:targetPackage="com.cyanogenmod.settings.device"
            		android:targetClass="com.cyanogenmod.settings.device.MimmiKeyboard" />
	    </PreferenceScreen>
	
	</PreferenceCategory>

</PreferenceScreen>
